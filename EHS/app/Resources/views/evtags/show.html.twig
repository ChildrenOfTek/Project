{% extends 'base.html.twig' %}

{% block section %}

    <div class="row">
      <div class="col-md-10 col-md-offset-1">
      <div class="panel panel-default">
        <div class="panel-heading">Tags d'évènements</div>
        <div class="panel-body">
<div class="container">
    <h1>Evtags</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ evtag.id }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ evtag.libelle }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('evtags_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('evtags_edit', { 'id': evtag.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
</div>

{% endblock %}
