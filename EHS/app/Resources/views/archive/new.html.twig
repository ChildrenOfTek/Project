{% extends 'base.html.twig' %}

{% block title %}
    Création d'archive
{% endblock %}

{% block section %}
<div class="row">
    <div class="col-md-10 col-md-offset-1">
        <div class="panel panel-default">
            <div class="panel-heading">Création d'une archive</div>
            <div class="panel-body">
                <div class="container col-md-10 col-md-offset-1">
{{ form_start(form) }}
        {{ form_row(form.titreArchive) }}
<div class="vich-file">
    {{ form_widget(form.imageFile) }}
</div>
<hr>
<table>
<tr>
    <td>
        <input class="button" type="submit" value="Créer une archive" />
        {{ form_end(form) }}
    </td>
    <td>
        <div class="buttonLink">
            <a href="{{ path('association_archive_index') }}">Retour à la liste</a>
        </div>
    </td>
</tr>
</table>



</div></div></div></div></div>

{% endblock %}
